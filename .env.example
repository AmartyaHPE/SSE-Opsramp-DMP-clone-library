# OpsRamp API Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env file to version control

# POD1 Configuration (Source)
POD1_BASE_URL=https://hpe-dev.api.try.opsramp.com
POD1_CLIENT_KEY=your_pod1_oauth_key_here
POD1_CLIENT_SECRET=your_pod1_oauth_secret_here
POD1_PARTNER_ID=your_pod1_partner_id_here
POD1_CLIENT_ID=your_pod1_client_tenant_id_here

# POD2 Configuration (Destination)
POD2_BASE_URL=https://hpe-dev.api.try.opsramp.com
POD2_CLIENT_KEY=your_pod2_oauth_key_here
POD2_CLIENT_SECRET=your_pod2_oauth_secret_here
POD2_PARTNER_ID=your_pod2_partner_id_here
POD2_CLIENT_ID=your_pod2_client_tenant_id_here

# Optional: For single POD testing
OPSRAMP_BASE_URL=https://hpe-dev.api.try.opsramp.com
OPSRAMP_CLIENT_KEY=your_oauth_key_here
OPSRAMP_CLIENT_SECRET=your_oauth_secret_here
